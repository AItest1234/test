# WinSCP Extension Configuration
# Copy this file to .env and fill in your values

# Application Server Settings
WINSCP_EXT_APP_SERVER_HOST=192.168.1.100
WINSCP_EXT_APP_SERVER_PORT=22
WINSCP_EXT_APP_SERVER_USER=administrator
WINSCP_EXT_APP_SERVER_PASS=your_password_here
# WINSCP_EXT_APP_SERVER_KEY_FILE=/path/to/key

# Fusion Server Settings
WINSCP_EXT_FUSION_SERVER_HOST=192.168.1.50
WINSCP_EXT_FUSION_SERVER_PORT=22
WINSCP_EXT_FUSION_SERVER_USER=fusion
WINSCP_EXT_FUSION_SERVER_PASS=your_password_here

# Deployment Settings
WINSCP_EXT_APP_DEPLOYMENT_FOLDER=C:\MyApp\Deployment
WINSCP_EXT_PROJECT_NAME=MyProject
WINSCP_EXT_APPLICATION_FILE=Application.zip
WINSCP_EXT_APPLICATION_INCLUDE=.*(service|core|auth)\.dll$
WINSCP_EXT_APPLICATION_EXCLUDE=.*test.*
WINSCP_EXT_INSTRUMENTED_FOLDER=Instrumented

# Technology Stack (JAVA or DOTNET)
WINSCP_EXT_TECH_STACK=JAVA

# SSH Key Settings
# WINSCP_EXT_SSH_KEY_PASSPHRASE=optional_passphrase
WINSCP_EXT_SSH_KEY_BITS=4096

# Paths (optional - use defaults if not set)
# WINSCP_EXT_FUSION_INSIGHT_PATH=C:\FusionLiteInsight\FusionLiteProjectService
# WINSCP_EXT_FUSION_PROJECTS_PATH=C:\FusionLiteProjects

# Logging
WINSCP_EXT_LOG_LEVEL=INFO
WINSCP_EXT_DEBUG_MODE=false
